{
  "name": "wiry",
  "version": "0.1.0",
  "description": "An unopinionated dependency injection framework for node",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rm -fr build",
    "coverage": "./node_modules/.bin/nyc npm t",
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register tests"
  },
  "keywords": [
    "wiry",
    "ioc",
    "dependency-injection",
    "wiring",
    "container"
  ],
  "author": {
    "name": "Adrian Schneider",
    "email": "adrian@compiledintent.com",
    "url": "http://twitter.com/adrianschneider"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AdrianSchneider/wiry.git"
  },
  "license": "MIT",
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/mocha": "^2.2.41",
    "@types/underscore": "^1.8.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "mocha": "^3.3.0",
    "nyc": "^10.0.0",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": false
  }
}
